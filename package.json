{
  "name": "arrigo-dynamics-client",
  "version": "0.1.0",
  "description": "Arrigo dynamics client",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "RS Software AB",
  "license": "MIT",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "start": "tsc -w | nodemon lib/test/testscript.js --watch lib/**/*.js",
    "example": "tsc -w | nodemon lib/examples/index.js --watch lib/**/*.js",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json --fix",
    "build": "yarn tsc",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn test && yarn run lint",
    "preversion": "yarn run lint",
    "version": "yarn run format && git add -A src",
    "postversion": "git push && git push --tags",
    "deploy": "copy package.json lib/package.json && cd lib && npm publish && cd .."
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.16",
    "apollo-link-ws": "^1.0.19",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "node-fetch": "^2.6.0",
    "subscriptions-transport-ws": "^0.9.16",
    "websocket": "^1.0.30",
    "ws": "^7.1.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node-fetch": "^2.5.2",
    "@types/websocket": "0.0.40",
    "@types/ws": "^6.0.3",
    "jest": "^24.9.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.1"
  }
}
